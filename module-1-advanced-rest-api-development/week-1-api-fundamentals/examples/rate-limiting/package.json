{
  "name": "rate-limiting-throttling",
  "version": "1.0.0",
  "description": "Advanced rate limiting and throttling techniques for APIs (TypeScript)",
  "main": "dist/rate-limiting-example.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/rate-limiting-example.js",
    "dev": "ts-node src/rate-limiting-example.ts",
    "watch": "nodemon --exec ts-node src/rate-limiting-example.ts",
  "clean": "rm -rf dist",
  "type-check": "tsc --noEmit",
  "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "rate-limiting",
    "throttling",
    "express",
    "api",
    "security",
    "nodejs",
    "typescript",
    "redis",
    "token-bucket",
    "sliding-window"
  ],
  "author": "DMC Juniors",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^1.6.0",
    "redis": "^4.6.7",
    "rate-limiter-flexible": "^2.4.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
